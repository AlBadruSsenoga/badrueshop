		function session_check(){
			$.ajax({
				url: 'http://localhost/badrueshop/index.php/home/is_logged/',
				success: function(data) {
					if(data == 'yah!good'){}
					else if (data == 'nope!bad') {
						location.replace('http://localhost/badrueshop/');
					}
				},
				error: function(e) {
					console.log(e)
				}
			});
		}
				// Hide it after 3 seconds
